    "github-latest-release": "^0.1.1",
    "puppeteer": "^11.0.0"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "conventional-changelog": "^3.1.24",
    "release-it": "^14.11.8"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "config": {
